# Azure DevOps hosts configured per documentation at https://docs.microsoft.com/en-us/azure/devops/repos/git/use-ssh-keys-to-authenticate?view=azure-devops

# defaults for all
Host  *
      ForwardAgent yes
      ForwardX11 yes
      UseKeychain yes
    
Host  github.com
      HostName github.com
      IdentityFile ~/.ssh/tkeenon-personal-rsa
      IdentitiesOnly yes

Host  ssh.dev.azure.com
      HostName ssh.dev.azure.com
      IdentityFile ~/.ssh/tkeenon-fullsteam-rsa
      IdentitiesOnly yes